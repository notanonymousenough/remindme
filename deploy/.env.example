# Docker образы
GITHUB_REPO=yourusername/remind-me
TAG=latest

# База данных
DB_USER=postgres
DB_PASSWORD=changeme
DB_NAME=remind_me
DB_HOST=postgres
DB_PORT=5432
DATABASE_URL=postgresql+asyncpg://${DB_USER}:${DB_PASSWORD}@${DB_HOST}:${DB_PORT}/${DB_NAME}

APP_ADDRESS=0.0.0.0

# Redis
REDIS_URL=redis://redis:6379/0

# Temporal
TEMPORAL_HOST=temporal:7233
TEMPORAL_NAMESPACE=remind-me
TEMPORAL_TASK_QUEUE=remind-me-tasks

# Telegram
TELEGRAM_BOT_TOKEN=your_bot_token
BOT_TOKEN=your_bot_token

# Yandex GPT API
YANDEX_GPT_API_KEY=your_api_key

# Общие настройки
LOG_LEVEL=INFO
DEBUG=false
